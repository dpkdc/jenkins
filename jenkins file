pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/dpkdc/PythonPractise.git']])
            }
        }
     
        stage('Build') {
            steps {
                bat 'python dict_demo.py'
            }
        }
    }
}
